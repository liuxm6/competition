/* radio  */

/* 选对后的 对勾样式 （白色对勾 可根据UI需求自己修改） */
.btn_right radio .wx-radio-input.wx-radio-input-checked::before{
   background-color:greenyellow; 
   background-image: url(data:image/png;base64,);
   background-size: 100% 100%;
  /* border: 2rpx solid #f2f2f2; */
}

/* 选错后的 对勾样式 （白色对勾 可根据UI需求自己修改） */
.btn_error radio .wx-radio-input.wx-radio-input-checked::before{
   background-color:red; 
   background-image: url(data:image/png;base64,);
   background-size: 100% 100%;

}

/* 选对后的背景样式 */
.btn_right radio .wx-radio-input.wx-radio-input-checked{
   border: none;
   background: greenyellow;
}

/* 选错后的背景样式 */
.btn_error radio .wx-radio-input.wx-radio-input-checked{
   border: none;
   background: red;
}


.btn_error radio .wx-radio-input.wx-radio-input-checked::before,.btn_right radio .wx-radio-input.wx-radio-input-checked::before{
   border-radius: 50%;
   width: 50rpx; 
   height: 50rpx; 
   text-align: center;
   color:transparent; /* 对勾颜色 白色 */

   line-height: 50rpx;
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%, -50%) scale(1);
}

/* 未选中的背景样式 */
radio .wx-radio-input{
   border-radius: 50%;/* 圆角 */
   width: 50rpx;
   height: 50rpx;
}